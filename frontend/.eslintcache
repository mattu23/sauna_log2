sauna-map>\n                <v-list-item-action v-if=\"user && user.roles && (user.roles.some(role => role.name === 'admin') || log.user.id === user.id)\">\n                  <v-btn icon :to=\"`/list/${log.id}`\">\n                    <v-icon color=\"green\">mdi-pencil</v-icon>\n                  </v-btn>\n                  <v-btn icon @click=\"() => deleteLog(log.id)\">\n                    <v-icon color=\"red\">mdi-delete</v-icon>\n                  </v-btn>\n                </v-list-item-action>\n              </v-list-item>\n             </div>\n            </v-list-item-group>\n          </v-list>\n        </v-container>\n        <v-pagination\n          v-model=\"page\"\n          :length=\"totalPages\"\n          @input=\"getLogData\"\n          circle\n        ></v-pagination>\n      </v-card>\n    </v-layout>\n  </div>\n</template>\n\n\n\n<script>\nimport SaunaMap from '@/components/SaunaMap';\n\n\nexport default {\n  head() {\n    return {\n      title: 'Saunalog-List'\n    }\n  },\n  components: {\n    SaunaMap,\n  },\n  data() {\n    return {\n      saunaLogs: [],\n      user: {},\n      page: 1,\n      totalPages: 0,\n      itemsPerPage: 5,\n      userRoles: '', \n    };\n  },\n  created() {\n    this.getLogData();\n  },\n  async mounted() {\n    await this.fetchUserData();\n  },\n  methods: {\n    async getLogData() {\n      try {\n        const response = await this.$axios.get(`${process.env.API_ENDPOINT}/saunalog/all`, {\n          params: {\n            page: this.page,\n            limit: this.itemsPerPage\n          },\n          withCredentials: true\n        });\n        this.saunaLogs = response.data.logs;\n        this.totalPages = response.data.totalPages;\n      } catch(error) {\n        console.error(error);\n        alert('正常にデータを取得できませんでした。ログイン状態を確認してください。');\n      }\n    },\n    async fetchUserData() {\n      const response = await this.$axios.get(`${process.env.API_ENDPOINT}/getUser`);\n      this.user = response.data;\n      this.userRoles = this.user.roles.map(role => role.name).join(', ');\n    },\n    async deleteLog(id) {\n      if(confirm('本当にデータを削除しますか？')) {\n        try {\n          await this.$axios.delete(`${process.env.API_ENDPOINT}/saunalog/${id}`);\n          this.getLogData();\n          alert('データを削除しました。');\n        } catch(error) {\n          if(error.response && error.response.data && error.response.data.message) {\n            alert(`エラー： ${error.response.data.message}`)\n          }\n        }\n      }\n    },\n    async logout() {\n      try {\n        await this.$auth.logout();\n        alert('ログアウトしました。');\n        this.$router.push('/');\n      } catch(error) {\n        console.error(error);\n        if(error.response && error.response.data && error.response.data.message) {\n          alert(`エラー： ${error.response.data.message}`)\n        }\n      }\n    },\n    async deleteUser() {\n      if(confirm('本当にユーザーを削除しますか？')) {\n        try {\n          await this.$axios.delete(`${process.env.API_ENDPOINT}/delete-user/`);\n          alert('ユーザー情報を削除しました。トップページに戻ります。');\n          this.$router.push('/');\n        } catch(error) {\n          alert('削除できませんでした。もう一度お試しください。');\n        }\n      } else {\n        // ユーザーがキャンセルを選択した場合、何もしない\n      }\n    },\n  }\n}  \n</script>","/Users/matsushitahiroki/Documents/codefile/sauna_log2/sauna_log/frontend/pages/editUser.vue",["124","125","126","127"],[],"/Users/matsushitahiroki/Documents/codefile/sauna_log2/sauna_log/frontend/pages/editPassword.vue",["128","129","130","131","132","133"],[],"/Users/matsushitahiroki/Documents/codefile/sauna_log2/sauna_log/frontend/pages/list/_id.vue",["134","135"],[],"/Users/matsushitahiroki/Documents/codefile/sauna_log2/sauna_log/frontend/pages/index.vue",[],[],"/Users/matsushitahiroki/Documents/codefile/sauna_log2/sauna_log/frontend/components/SaunaMap.vue",["136","137","138","139","140","141"],[],"<template>\n  <div :id=\"'map-' + logId\" class=\"sauna-map\"></div>\n</template>\n\n<script>\n// SaunaMap.vu